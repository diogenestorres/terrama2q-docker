{
  "basePath": "{{ PUBLIC_PATH }}/admin/",
  "port": 36000,
  "defaultFilePathList": [
    "/data",
    "/shared-data"
  ],
  "defaultExecutablePath": "/opt/terrama2/%%TERRAMA2_TAG%%/bin",
  "defaultExecutableName": "terrama2_service",
  "disablePDF": true,
  "debug": true,
  "OGR2OGR": "ogr2ogr",
  "SHP2PGSQL": "shp2pgsql",
  "db": {
    "username": "{{ POSTGRES_USER }}",
    "password": "{{ POSTGRES_PASSWORD }}",
    "database": "{{ POSTGRES_DB }}",
    "host": "{{ POSTGRES_HOST }}",
    "port": {{ POSTGRES_PORT }},
    "dialect": "postgres",
    "define": {
      "schema": "terrama2"
    },
    "logging": false
  },
  "toolsMenu": [
    {
      "textEn": "Example",
      "textEs": "Ejemplo",
      "textFr": "Exemple",
      "textPt": "Exemplo",
      "url": "http://example.com/",
      "openInNewTab": true
    }
  ]
}
